#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/summer-gonner/traffica-front-vue3/node_modules/.pnpm/tsup@8.3.0_jiti@2.1.2_postcss@8.4.47_tsx@4.19.1_typescript@5.6.3_yaml@2.5.1/node_modules/tsup/dist/node_modules:/mnt/d/summer-gonner/traffica-front-vue3/node_modules/.pnpm/tsup@8.3.0_jiti@2.1.2_postcss@8.4.47_tsx@4.19.1_typescript@5.6.3_yaml@2.5.1/node_modules/tsup/node_modules:/mnt/d/summer-gonner/traffica-front-vue3/node_modules/.pnpm/tsup@8.3.0_jiti@2.1.2_postcss@8.4.47_tsx@4.19.1_typescript@5.6.3_yaml@2.5.1/node_modules:/mnt/d/summer-gonner/traffica-front-vue3/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/summer-gonner/traffica-front-vue3/node_modules/.pnpm/tsup@8.3.0_jiti@2.1.2_postcss@8.4.47_tsx@4.19.1_typescript@5.6.3_yaml@2.5.1/node_modules/tsup/dist/node_modules:/mnt/d/summer-gonner/traffica-front-vue3/node_modules/.pnpm/tsup@8.3.0_jiti@2.1.2_postcss@8.4.47_tsx@4.19.1_typescript@5.6.3_yaml@2.5.1/node_modules/tsup/node_modules:/mnt/d/summer-gonner/traffica-front-vue3/node_modules/.pnpm/tsup@8.3.0_jiti@2.1.2_postcss@8.4.47_tsx@4.19.1_typescript@5.6.3_yaml@2.5.1/node_modules:/mnt/d/summer-gonner/traffica-front-vue3/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tsup/dist/cli-default.js" "$@"
else
  exec node  "$basedir/../tsup/dist/cli-default.js" "$@"
fi
